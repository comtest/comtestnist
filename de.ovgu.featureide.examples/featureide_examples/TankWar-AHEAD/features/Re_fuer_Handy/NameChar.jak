public class NameChar {
	int charvalue;

	public NameChar(char name) {
		this.charvalue = name;
	}

	public String getString() {
		return (char) charvalue + "";
	}

	public void up() {
		charvalue++;
		if (charvalue > 90) {
			charvalue = 65;
		}
	}

	public void down() {
		charvalue--;
		if (charvalue < 65) {
			charvalue = 90;
		}
	}
}
