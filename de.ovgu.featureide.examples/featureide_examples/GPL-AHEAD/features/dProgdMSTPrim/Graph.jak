refines class Graph {
    // Executes MSTPrim
    public void run( Vertex s )
     {
        Graph gaux = Prim( s );
        Graph.stopProfile();
        gaux.display();
        Graph.resumeProfile();
        Super( Vertex ).run( s );
    }
}
